&soc {
       i2c_9:i2c@9 {
		cell-index = <9>;
		compatible = "i2c-gpio";
		i2c-gpio-scl = <&msm_gpio 76 0x00>;
		i2c-gpio-sda = <&msm_gpio 75 0x00>;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		max77849@66 {
			compatible = "max77849,i2c";
			reg = <0x66>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <62 0>;
			max77849,irq-gpio = <&msm_gpio 62 0x00>;
			pinctrl-names = "muic_i2c_active","muic_i2c_suspend";
			pinctrl-0 = <&muic_i2c_active &muic_int_pin>;
			pinctrl-1 = <&muic_i2c_suspend &muic_int_pin>;
		};
	};
};
